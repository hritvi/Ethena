<body oncontextmenu="return false;">
	

	<div class="wrapper">

		<header class="topnav">
		  <a href="#" class="navbar-company-name">
		    AppName
		  </a>
		  <a href="/" class="navbar-right-navigation">
		    LogOut
		  </a>
		</header>

		<main>
			<div class="main-section">
				<div class="container">
					<div class="main-section-data">
						<div class="row">
							<div class="col-lg-3 col-md-4 pd-left-none no-pd">
								<div class="main-left-sidebar no-margin">
									<div class="user-data full-width">
										<div class="user-profile">
											<div class="username-dt">
												<div class="usr-pic">
													<img src="images/resources/user-pic.png" alt="">
												</div>
											</div><!--username-dt end-->
											<div class="user-specs">
												<h3><%= user.name %></h3>
											</div>
										</div><!--user-profile end-->
										<ul class="user-fw-status">
											<li>
												<h4>Prisma</h4>
												<span id="prismaCount"><%= user.prisma %></span>
											</li>
										</ul>
									</div><!--user-data end-->
								</div><!--main-left-sidebar end-->
							</div>
							<div class="col-lg-6 col-md-8 no-pd">
								<div class="main-ws-sec">
									<div class="post-topbar">
										<div class="user-picy">
											<img src="images/resources/user-pic.png" alt="">
										</div>
										<div class="post-st">
											<ul>
												<li><a class="post_project active" href="#" title="">Add a achievement</a></li>
												<li><a class="post-jb active" href="#" title="">Praise Someone Else</a></li>
											</ul>
										</div><!--post-st end-->
									</div><!--post-topbar end-->
									<div class="posts-section">
										<% for (var i=0;i< data.length;i++){ %>
										<div class="post-bar">
											<div class="post_topbar">
												<div class="usy-dt">
													<img src="images/resources/us-pic.png" alt="">
													<div class="usy-name">
														<h3><%= data[i].user %>
														<% if(data[i].user != data[i].addressedUser) { %>
														<span class= "fa fa-chevron-right" style="padding: 0 3px;"></span><%= data[i].addressedUser %></h3>
														<% } %>
													</div>
												</div>
											</div>
											<div class="job_descp">
												<p  id = '<%= "issue-" + i %>'><%= data[i].issue %></p>
											</div>
											<div class="job-status-bar">
												<ul class="like-com">
													<li>
														<button class="clap" id = '<%= "upvote-" + i %>' onclick="voteForCandidate(this.id)"><span>
														    <!--  SVG Created by Luis Durazo from the Noun Project  -->
														    <svg id="clap--icon" xmlns="http://www.w3.org/2000/svg" viewBox="-549 338 100.1 125">
														  <path d="M-471.2 366.8c1.2 1.1 1.9 2.6 2.3 4.1.4-.3.8-.5 1.2-.7 1-1.9.7-4.3-1-5.9-2-1.9-5.2-1.9-7.2.1l-.2.2c1.8.1 3.6.9 4.9 2.2zm-28.8 14c.4.9.7 1.9.8 3.1l16.5-16.9c.6-.6 1.4-1.1 2.1-1.5 1-1.9.7-4.4-.9-6-2-1.9-5.2-1.9-7.2.1l-15.5 15.9c2.3 2.2 3.1 3 4.2 5.3zm-38.9 39.7c-.1-8.9 3.2-17.2 9.4-23.6l18.6-19c.7-2 .5-4.1-.1-5.3-.8-1.8-1.3-2.3-3.6-4.5l-20.9 21.4c-10.6 10.8-11.2 27.6-2.3 39.3-.6-2.6-1-5.4-1.1-8.3z"/>
														  <path d="M-527.2 399.1l20.9-21.4c2.2 2.2 2.7 2.6 3.5 4.5.8 1.8 1 5.4-1.6 8l-11.8 12.2c-.5.5-.4 1.2 0 1.7.5.5 1.2.5 1.7 0l34-35c1.9-2 5.2-2.1 7.2-.1 2 1.9 2 5.2.1 7.2l-24.7 25.3c-.5.5-.4 1.2 0 1.7.5.5 1.2.5 1.7 0l28.5-29.3c2-2 5.2-2 7.1-.1 2 1.9 2 5.1.1 7.1l-28.5 29.3c-.5.5-.4 1.2 0 1.7.5.5 1.2.4 1.7 0l24.7-25.3c1.9-2 5.1-2.1 7.1-.1 2 1.9 2 5.2.1 7.2l-24.7 25.3c-.5.5-.4 1.2 0 1.7.5.5 1.2.5 1.7 0l14.6-15c2-2 5.2-2 7.2-.1 2 2 2.1 5.2.1 7.2l-27.6 28.4c-11.6 11.9-30.6 12.2-42.5.6-12-11.7-12.2-30.8-.6-42.7m18.1-48.4l-.7 4.9-2.2-4.4m7.6.9l-3.7 3.4 1.2-4.8m5.5 4.7l-4.8 1.6 3.1-3.9"/>
														</svg>
														  </span>
														  <span id="clap--count" class="clap--count"></span>
														  <span id='<%= "votes-" + i %>' class="clap--count-total"></span></button>
													</li> 
												</ul>
											</div>
										</div><!--post-bar end-->
										<% } %>
									</div><!--posts-section end-->
								</div><!--main-ws-sec end-->
							</div>
							<div class="col-lg-3 pd-right-none no-pd">
								<div class="right-sidebar">
									<div class="widget widget-about">
										<img src="images/wd-logo.png" alt="">
										<h3>Prismatic Possessor</h3>
										<div class="sign_link">
											<a href="/leaderboard">View More</a>
										</div>
									</div><!--widget-about end-->
									<div class="widget widget-about">
										<img src="images/wd-logo.png" alt="">
										<h3>Tantalizing TouchStone</h3>
										<div class="sign_link">
											<a href="/leaderboard">View More</a>
										</div>
									</div><!--widget-about end-->
									<div class="widget widget-about">
										<img src="images/wd-logo.png" alt="">
										<h3>Ardent Achiever</h3>
										<div class="sign_link">
											<a href="/leaderboard">View More</a>
										</div>
									</div><!--widget-about end-->
								</div><!--right-sidebar end-->
							</div>
						</div>
					</div><!-- main-section-data end-->
				</div> 
			</div>
		</main>




		<div class="post-popup pst-pj">
			<div class="post-project">
				<h3>Add a achievement</h3>
				<div class="post-project-fields">
					<form action="/submit" method="POST">
						<div class="row">
							<input type="hidden" name="handle" value="<%= user.name %>">
							<div class="col-lg-12">
								<textarea name="issue" placeholder="Description"></textarea>
							</div>
							<input type="hidden" name='userid' value="<%= user.id %>">
				            <input type="hidden" name='username' value="<%= user.name %>">
				            <input type="hidden" name='prisma' value="<%= user.prisma %>">
				            <div class="col-lg-12">
								<ul>
									<li><button class="active" type="submit" value="post">Post</button></li>
									<li><a href="#" title="">Cancel</a></li>
								</ul>
							</div>
							
						</div>
					</form>
				</div><!--post-project-fields end-->
				<a href="#" title=""><i class="fa fa-times"></i></a>
			</div><!--post-project end-->
		</div><!--post-project-popup end-->

		<div class="post-popup job_post">
			<div class="post-project">
				<h3>Praise Someone Else</h3>
				<div class="post-project-fields">
					<form action="/submit" method="POST">
						<div class="row">
							<div class="col-lg-12">
								<input type="text" name="handle" placeholder="username">
							</div>
							<div class="col-lg-12">
								<textarea name="issue" placeholder="Description"></textarea>
							</div>
							<input type="hidden" name='userid' value="<%= user.id %>">
				            <input type="hidden" name='username' value="<%= user.name %>">
				            <input type="hidden" name='prisma' value="<%= user.prisma %>">
				            <div class="col-lg-12">
								<ul>
									<li><button class="active" type="submit" value="post">Post</button></li>
									<li><a href="#" title="">Cancel</a></li>
								</ul>
							</div>
							
						</div>
					</form>
				</div><!--post-project-fields end-->
				<a href="#" title=""><i class="fa fa-times"></i></a>
			</div><!--post-project end-->
		</div><!--post-project-popup end-->
	</div><!--theme-layout end-->
