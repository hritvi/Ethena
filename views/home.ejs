<style>
	.diamond {
		margin-left: 3rem;
		margin-top: -0.4rem;
	}

	button {
	  position: absolute;
	  top: 0;
	  left: 0;
	  right: 0;
	  bottom: 0;
	  margin: auto;
	  width: 90px;
	  height: 40px;
	  background: #FFFFFF;
	  border: 0;
	  font-size: .9rem;
	  font-family: Futura;
	  transition: all .2s ease-in-out;
	  cursor: pointer;
	  border-radius: 6px;
	  box-shadow: 0px 2px 0px rgba(0, 0, 0, 0.1);
	  outline: none;
	}
	button:hover {
	  color: #84d9ff;
	  -webkit-transform: scale(1.2);
	          transform: scale(1.2);
	}
	button:hover span {
	  opacity: 1;
	  transition: all .2s ease-in-out;
	}
	button:active {
	  -webkit-transform: scale(1.4);
	          transform: scale(1.4);
	}
	button span {
	  position: absolute;
	  top: -55px;
	  left: 0;
	  right: 0;
	  margin: auto;
	  background: #84d9ff;
	  color: rgba(0, 0, 0, 0.5);
	  padding: 10px 0;
	  border-radius: 100px;
	  opacity: 0;
	  transition: all .2s ease-in-out;
	}
	button span:hover {
	  opacity: 1;
	}
	button span:after {
	  position: absolute;
	  bottom: -7px;
	  left: 0;
	  right: 0;
	  margin: auto;
	  content: "";
	  width: 0;
	  height: 0;
	  border-left: 8px solid transparent;
	  border-right: 8px solid transparent;
	  border-top: 8px solid #84d9ff;
	}
</style>
</head>


<body oncontextmenu="return false;">
	

	<div class="wrapper">

		<header class="topnav">
		  <a href="#" class="navbar-company-name">
		    AppName
		  </a>
		  <a href="/" class="navbar-right-navigation">
		    LogOut
		  </a>
		</header>

		<main>
			<div class="main-section">
				<div class="container">
					<div class="main-section-data">
						<div class="row">
							<div class="col-lg-3 col-md-4 pd-left-none no-pd">
								<div class="main-left-sidebar no-margin">
									<div class="user-data full-width">
										<div class="user-profile">
											<div class="username-dt">
												<div class="usr-pic">
													<img src="images/resources/user-pic.png" alt="">
												</div>
											</div><!--username-dt end-->
											<div class="user-specs">
												<h3><%= user.name %></h3>
											</div>
										</div><!--user-profile end-->
										<ul class="user-fw-status">
											<li>
												<h4>Prisma</h4>
												<span id="prismaCount"><img src="images/diamond.jpeg" height="36px" width="36px" class="diamond"><%= user.prisma %></span>
											</li>
										</ul>
									</div><!--user-data end-->
								</div><!--main-left-sidebar end-->
							</div>
							<div class="col-lg-6 col-md-8 no-pd">
								<div class="main-ws-sec">
									<div class="post-topbar">
										<div class="user-picy">
											<img src="images/resources/user-pic.png" alt="">
										</div>
										<div class="post-st">
											<ul>
												<li><a class="post_project active" href="#" title="">Add a achievement</a></li>
												<li><a class="post-jb active" href="#" title="">Praise Someone Else</a></li>
											</ul>
										</div><!--post-st end-->
									</div><!--post-topbar end-->
									<div class="posts-section">
										<% for (var i=0;i< data.length;i++){ %>
										<div class="post-bar">
											<div class="post_topbar">
												<div class="usy-dt">
													<img src="images/resources/us-pic.png" alt="">
													<div class="usy-name">
														<h3><%= data[i].user %>
														<% if(data[i].user != data[i].addressedUser) { %>
														<span class= "fa fa-chevron-right" style="padding: 0 3px;"></span><%= data[i].addressedUser %></h3>
														<% } %>
													</div>
												</div>
											</div>
											<div class="job_descp">
												<p  id = '<%= "issue-" + i %>'><%= data[i].issue %></p>
											</div>
											<div class="job-status-bar">
												<ul class="like-com">
													<li>
														<button type='button' id = '<%= "upvote-" + i %>' onclick="voteForCandidate(this.id)">
														  <i class='fa fa-thumbs-up'></i>
														  Like
														  <span>0</span>
														</button>
													</li> 
												</ul>
											</div>
										</div><!--post-bar end-->
										<% } %>
									</div><!--posts-section end-->
								</div><!--main-ws-sec end-->
							</div>
							<div class="col-lg-3 pd-right-none no-pd">
								<div class="right-sidebar">
									<div class="widget widget-about">
										<img src="images/wd-logo.png" alt="">
										<h3>Prismatic Possessor</h3>
										<div class="sign_link">
											<h3><a href="#" title=""><%= topPrisma.username %></a></h3>
											<a href="/leaderboard">View More</a>
										</div>
									</div><!--widget-about end-->
									<div class="widget widget-about">
										<img src="images/wd-logo.png" alt="">
										<h3>Tantalizing TouchStone</h3>
										<div class="sign_link">
											<a href="/leaderboard">View More</a>
										</div>
									</div><!--widget-about end-->
									<div class="widget widget-about">
										<img src="images/wd-logo.png" alt="">
										<h3>Ardent Achiever</h3>
										<div class="sign_link">
											<h3><a href="#" title=""><%= topVote.username %></a></h3>
											<a href="/leaderboard">View More</a>
										</div>
									</div><!--widget-about end-->
								</div><!--right-sidebar end-->
							</div>
						</div>
					</div><!-- main-section-data end-->
				</div> 
			</div>
		</main>




		<div class="post-popup pst-pj">
			<div class="post-project">
				<h3>Add a achievement</h3>
				<div class="post-project-fields">
					<form action="/submit" method="POST">
						<div class="row">
							<input type="hidden" name="handle" value="<%= user.name %>">
							<div class="col-lg-12">
								<textarea name="issue" placeholder="Description"></textarea>
							</div>
							<input type="hidden" name='userid' value="<%= user.id %>">
				            <input type="hidden" name='username' value="<%= user.name %>">
				            <input type="hidden" name='prisma' value="<%= user.prisma %>">
				            <div class="col-lg-12">
								<ul>
									<li><button class="active" type="submit" value="post">Post</button></li>
									<li><a href="#" title="">Cancel</a></li>
								</ul>
							</div>
							
						</div>
					</form>
				</div><!--post-project-fields end-->
				<a href="#" title=""><i class="fa fa-times"></i></a>
			</div><!--post-project end-->
		</div><!--post-project-popup end-->

		<div class="post-popup job_post">
			<div class="post-project">
				<h3>Praise Someone Else</h3>
				<div class="post-project-fields">
					<form action="/submit" method="POST">
						<div class="row">
							<div class="col-lg-12">
								<input type="text" name="handle" placeholder="username">
							</div>
							<div class="col-lg-12">
								<textarea name="issue" placeholder="Description"></textarea>
							</div>
							<input type="hidden" name='userid' value="<%= user.id %>">
				            <input type="hidden" name='username' value="<%= user.name %>">
				            <input type="hidden" name='prisma' value="<%= user.prisma %>">
				            <div class="col-lg-12">
								<ul>
									<li><button class="active" type="submit" value="post">Post</button></li>
									<li><a href="#" title="">Cancel</a></li>
								</ul>
							</div>
							
						</div>
					</form>
				</div><!--post-project-fields end-->
				<a href="#" title=""><i class="fa fa-times"></i></a>
			</div><!--post-project end-->
		</div><!--post-project-popup end-->
	</div><!--theme-layout end-->
